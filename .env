# Development Environment Variables
# Copy to .env and fill in your actual values

# Backend Server
PORT=5000
FRONTEND_URL=http://localhost:5173

# Stripe Keys (Get from https://dashboard.stripe.com/test/apikeys)
STRIPE_SECRET_KEY=sk_test_YOUR_SECRET_KEY_HERE
STRIPE_WEBHOOK_SECRET=whsec_YOUR_WEBHOOK_SECRET_HERE

# Stripe Price IDs (Create in Stripe Dashboard > Products > Add Product)
# Professional Monthly ($49/month)
STRIPE_PRICE_PRO_MONTHLY=price_YOUR_MONTHLY_PRICE_ID
# Professional Annual ($42/month billed annually)
STRIPE_PRICE_PRO_ANNUAL=price_YOUR_ANNUAL_PRICE_ID

# Frontend (Vite)
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_YOUR_PUBLISHABLE_KEY_HERE
VITE_API_URL=http://localhost:5000
